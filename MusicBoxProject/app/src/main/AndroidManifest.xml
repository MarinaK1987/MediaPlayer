<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicboxproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HouseMusicActivity" android:label="@string/bHouseMusic"/>
        <activity android:name=".MetalMusicActivity" android:label="@string/bMetalMusic"/>
        <activity android:name=".PopMusicActivity" android:label="@string/bPopMusic"/>
        <activity android:name=".RockMusicActivity" android:label="@string/bRockMusic"></activity>
    </application>

</manifest>